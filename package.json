{
  "name": "bun-express-apiato",
  "version": "1.0.0",
  "description": "Express.js API with Apiato for Bun.js",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "build": "tsc",
    "swagger:generate": "bun run src/scripts/generate-swagger.ts",
    "swagger:watch": "bun --watch run src/scripts/generate-swagger.ts",
    "generate:module": "bun run src/libs/apiato/scripts/generate.ts",
    "generate:plugin": "bun run src/libs/apiato/scripts/generate-plugin.ts",
    "generate:socket": "bun run src/libs/apiato/scripts/generate-socket.ts",
    "generate:template": "node scripts/generator.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/client-ses": "^3.750.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@hapi/boom": "^10.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@whiskeysockets/baileys": "^6.7.13",
    "apiato": "^3.0.0",
    "axios": "^1.7.9",
    "canvas": "^3.1.0",
    "cors": "^2.8.5",
    "docx": "^9.2.0",
    "dropbox-sdk": "^0.1.1",
    "express": "^4.18.2",
    "firebase": "^11.3.1",
    "firebase-admin": "^13.1.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "joi": "^17.13.3",
    "jsbarcode": "^3.11.6",
    "mongoose": "^8.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "pino": "^9.6.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^17.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.4.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "bun-types": "latest",
    "dotenv": "^16.4.7",
    "openai": "^4.85.4",
    "typescript": "^5.3.3"
  }
}
